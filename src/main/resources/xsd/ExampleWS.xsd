<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns="http://example.com/ws/example"
           xmlns:dom="http://example.com/domain/types"
           targetNamespace="http://example.com/ws/example" elementFormDefault="qualified" attributeFormDefault="unqualified">

    <xs:import schemaLocation="ExampleRequest.xsd" namespace="http://example.com/domain/types"/>

    <xs:element name="ExampleRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="message" type="dom:ExampleRequestType" maxOccurs="100"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>
